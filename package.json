{
  "name": "akpaddyy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": "24.11.0",
    "pnpm": "10.17.1"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src drizzle.config.ts",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "test": "node scripts/test.js",
    "migrate": "tsx src/scripts/migrate.ts",
    "deploy:commands": "tsx src/scripts/deploy-commands.ts",
    "setup": "pnpm migrate && pnpm deploy:commands"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "fastify": "^5.6.2",
    "mysql2": "^3.16.0",
    "neverthrow": "^8.2.0",
    "p-queue": "^9.1.0",
    "p-retry": "^7.1.1",
    "pino": "^10.1.1",
    "ulid": "^3.0.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  }
}
